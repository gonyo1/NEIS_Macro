       <property name="styleSheet">
        <string notr="true">/* Macro grid */
#Macrobar_grid {margin: 0px}

/* Macro buttons in grid */
#Macrobar_grid .QWidget {
	margin: 0px; 
	border-radius: 25px; 
	background: rgba(230, 111, 85, 255);
}
#Macrobar_grid .QWidget:hover {
	background:  rgba(230, 111, 85, 200)
}
#Macrobar_grid .QWidget .QLabel {
	font: 18px &quot;나눔스퀘어 Bold&quot;; 
	color: white;
}
#Macrobar_grid .QWidget .QPushButton {
	background: transparent;
}


/* Macro scrollbar */
#Macrobar_scroll {
	margin: 0px; 
	padding: 0px; 
	border: 0px solid;
}
.QScrollArea .QScrollBar:vertical {
	border: 0px solid #c3c3c3;
	background:white;
	width:10px;
	margin: 0px 0px 0px 2px;
}
.QScrollArea .QScrollBar::handle:vertical {
	min-height: 0px;
	border: 0px solid red;
	border-radius: 4px;
	background-color: #c3c3c3;
}
.QScrollArea .QScrollBar::add-line:vertical {       
	height: 0px;
	subcontrol-position: bottom;
	subcontrol-origin: margin;
}
.QScrollArea .QScrollBar::sub-line:vertical {
	height: 0 px;
	subcontrol-position: top;
	subcontrol-origin: margin;
}</string>
